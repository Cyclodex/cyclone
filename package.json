{
  "name": "cyclone",
  "author": "Cyclodex",
  "version": "1.0.0-beta2",
  "description": "Time tracking",
  "scripts": {
    "start": "node_modules/webpack/bin/webpack.js --watch",
    "build dev": "node_modules/webpack/bin/webpack.js",
    "build staging": "node_modules/webpack/bin/webpack.js --config webpack.config.staging.js --progress",
    "build production": "node_modules/webpack/bin/webpack.js --config webpack.config.production.js --progress",
    "deploy staging": "firebase use staging; firebase deploy",
    "deploy production": "firebase use production; firebase deploy",
    "version": "./version.sh $npm_package_version",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cyclodex/cyclone.git"
  },
  "license": "GPL-3.0+",
  "bugs": {
    "url": "https://github.com/Cyclodex/cyclone/issues"
  },
  "homepage": "https://github.com/Cyclodex/cyclone#readme",
  "dependencies": {
    "angular": "1.5.8",
    "angular-animate": "1.5.3",
    "angular-aria": "1.5.3",
    "angular-clipboard": "1.5.0",
    "angular-loading-bar": "^0.9.0",
    "angular-material": "http://github.com/angular/bower-material/tarball/v1.1.4",
    "angular-messages": "1.5.3",
    "angular-moment": "^1.0.1",
    "angular-ui-router": "1.0.0-rc.1",
    "angularfire": "2.0.2",
    "firebase": "3.3.2",
    "firebaseui": "^1.0.0",
    "randomcolor": "^0.4.4",
    "angulartics-google-analytics": "0.4.0"
  },
  "devDependencies": {
